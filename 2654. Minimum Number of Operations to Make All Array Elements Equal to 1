class Solution {
    public int minOperations(int[] nums) {
        int ones = 0;
        int numsGcd = 0;
        for(int num : nums){
            if(num == 1){
                ones++;
            }
            numsGcd = gcd(numsGcd,num);
        }
        if(ones > 0){
            return nums.length - ones;
        }
        if(numsGcd > 1){
            return -1;
        }
        int min = nums.length;
        for(int i = 0; i < nums.length; i++){
            numsGcd = 0;
            for(int j = i; j < nums.length; j++){
                numsGcd = gcd(numsGcd, nums[j]);
                if(numsGcd == 1){
                    min = Math.min(min, j-i+1);
                    break;
                }
            }
        }
        return min+nums.length-2;
    }
    private int gcd(int a, int b){
        if(b!=0){
            return gcd(b, a%b);
        }
        return a;
    }
}
