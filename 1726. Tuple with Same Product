class Solution {
    public int tupleSameProduct(int[] nums) {
        Arrays.sort(nums);
        int count=0;
        for(int i=0;i<nums.length-1;i++){
            for(int j=nums.length-1;j>i;j--){
                Set<Integer> set=new HashSet<>();
                for(int k=i+1;k<j;k++){
                    if((nums[i]*nums[j])%nums[k]==0){
                        if(set.contains((nums[i]*nums[j])/nums[k])){
                            count++;
                        }
                        set.add(nums[k]);
                    }
                }
            }
        }
        return count*8;
    }
}
