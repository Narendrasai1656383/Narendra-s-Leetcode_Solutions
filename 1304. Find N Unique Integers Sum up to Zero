class Solution {
    public int[] sumZero(int n) {
        if(n==1){
            return new int[1];
        }
        int min=0-n/2;
        int max=n/2;
        int[] uniqueArr=new int[n];
        for(int i=0;i<n/2;i++){
            uniqueArr[i]=min++;
            uniqueArr[n-i-1]=max--;
        }
        if(n%2==1){
            uniqueArr[n/2]=0;
        }
        return uniqueArr;
    }
}
