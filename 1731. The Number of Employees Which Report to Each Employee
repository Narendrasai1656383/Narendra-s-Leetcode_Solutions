# Write your MySQL query statement below
SELECT em.REPORTS_TO AS EMPLOYEE_ID, e.NAME, COUNT(em.EMPLOYEE_ID) AS REPORTS_COUNT, ROUND(AVG(em.AGE)) AS AVERAGE_AGE FROM EMPLOYEES em LEFT JOIN EMPLOYEES e ON em.REPORTS_TO = e.EMPLOYEE_ID WHERE em.REPORTS_TO IS NOT NULL GROUP BY em.REPORTS_TO ORDER BY em.REPORTS_TO;
