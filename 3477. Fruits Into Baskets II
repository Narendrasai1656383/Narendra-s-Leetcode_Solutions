class Solution {
    public int numOfUnplacedFruits(int[] fruits, int[] baskets) {
        int placedFruits=0;
        for(int fruit:fruits){
            for(int i=0;i<baskets.length;i++){
                if(baskets[i]>=fruit){
                    baskets[i]=0;
                    placedFruits++;
                    break;
                }
            }
        }
        return baskets.length-placedFruits;
    }
}
