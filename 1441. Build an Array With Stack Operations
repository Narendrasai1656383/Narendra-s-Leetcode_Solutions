class Solution {
    public List<String> buildArray(int[] target, int n) {
        List<String> res = new ArrayList<>();
        int temp = 1,i = 0;
        for(i=0; i<target.length && temp<=n; ){
            res.add("Push");
            if(target[i] == temp){
                i++;
            }else{
                res.add("Pop");
            }
            temp++;
        }
        return res;
    }
}
