class Solution {
    public int numSubmat(int[][] mat) {
        int subMatrices=0;
        int[][] dp=new int[mat.length][mat[0].length];
        for(int i=0;i<mat.length;i++){
            for(int j=0;j<mat[0].length;j++){
                if(j==0){
                    dp[i][j]=mat[i][j];
                }else{
                    dp[i][j]=mat[i][j]==1?dp[i][j-1]+1:0;
                }
                int cur=dp[i][j];
                for(int k=i;k>=0;k--){
                    cur=Math.min(cur,dp[k][j]);
                    if(cur==0){
                        break;
                    }
                    subMatrices+=cur;
                }
            }
        }
        return subMatrices;
    }
}
